{
    "manifest_version": 2,
    "name": "Foxium Blocker",
    "version": "1.0",
    "description": "Removes cookie popups from certain websites.",
    "icons": {
        "48": "icon.webp"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.google.com/*"
            ],
            "js": [
                "google.js"
            ]
        }
    ]
}