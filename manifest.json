{
    "manifest_version": 2,
    "name": "Foxium Blocker",
    "version": "1.0",
    "description": "Removes cookie popups from certain websites.",
    "icons": {
        "48": "icon.webp"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}