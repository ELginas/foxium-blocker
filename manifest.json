{
    "manifest_version": 2,
    "name": "Foxium Blocker",
    "version": "1.0",
    "description": "Removes cookie popups from certain websites.",
    "icons": {
        "48": "icon.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "https://*.google.com/*"
    ],
    "background": {
        "scripts": ["background.js"]
    }
}
